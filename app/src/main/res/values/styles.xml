<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="reservation_item_labels">
        <item name="android:fontFamily">@font/franklingothicheavyregular</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="reservation_item_values">
        <item name="android:fontFamily">@font/frabk</item>
        <item name="android:textColor">@color/vallarta_blue</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="auto_search">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:dropDownWidth">match_parent</item>
        <item name="android:drawableEnd">@drawable/ic_dropdown_arrow</item>
    </style>

    <style name="popup_labels">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">2dp</item>
    </style>
</resources>